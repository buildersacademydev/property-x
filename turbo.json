{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  },
   "globalEnv": [
      "DATABASE_URL_MARKETING",
      "RESEND_API_KEY",
      "DATABASE_URL",
      "NODE_ENV",
      "UPSTASH_REDIS_REST_URL",
      "UPSTASH_REDIS_REST_TOKEN",
      "NEXT_PUBLIC_CONTRACT_ADDRESS",
      "NEXT_PUBLIC_NETWORK",
      "NEXT_PUBLIC_MARKETPLACE",
      "NEXT_PUBLIC_FULFILL",
      "NEXT_PUBLIC_ADMIN",
      "NEXT_PUBLIC_STAKE"
  ]
}
